.row
  .col-md-8.offset-md-2
    %h1 My Uploaded Keywords
    %table.table.table-striped
      %thead
        %tr
          = render 'headers'
      %tbody
        - @keywords.each do |keyword|
          %tr{id: dom_id(keyword)}
            = render partial: 'infor', locals: { keyword: keyword, include_link: true }

.row
  .col-md-8.offset-md-2
    = link_to "Upload a new keyword file", new_keyword_path, class: "btn btn-primary"
